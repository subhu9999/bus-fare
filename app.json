{
  "expo": {
    "name": "Bus Fare Calculator",
    "slug": "bus-fare-calculator",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 15000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "notification": {
      "icon": "./assets/icon-notification.png",
      "color":"#FF0D0D",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} New Notification"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mumbai.busfarecalculator",
      "versionCode": 7,
      "googleServicesFile": "./google-services.json",
      "config":{
        "googleMobileAdsAppId": "ca-app-pub-7589498491080333~2369849870"
      },
      "permissions":[]
    }
  }
}
